class Vehicle:
    COLOR_VARIANTS=['blue', 'red', 'green', 'black', 'white']
    def __init__(self, owner, __model, __color, __engine_power ):
        self.owner = owner
        self.model = __model
        self.engine_power = __engine_power
        self.color = __color

    def get_model(self):
        return f'Модель: {self.model}'

    def get_horsepower(self):
        return f'Мощность двигателя: {self.engine_power}'


    def get_color(self):
        return f'Цвет: {self.color}'


    def print_info(self):
        print (result1)
        print (result2)
        print(result3)
        print("Владелец:", self.owner)
        return "OK"


class Sedan(Vehicle):
    PASSENGERS_LIMIT = 5
    def __init__(self, owner, model, color, engine_power ):
        self.owner = owner
        self.model = model
        self.engine_power = engine_power
        self.color = color
    def set_color(self, new_color):
        self.new_color = new_color

        if self.new_color.upper:
            self.new_color = self.new_color.lower()

        if self.new_color in self.COLOR_VARIANTS:
            self.color = self.new_color
            return f'Цвет: {self.color}'

        else:
            print(f'Нельзя сменить цвет на {self.new_color}')




v1 = Sedan('Fedos', 'Toyota Mark II', 'blue', 500)

result1 = v1.get_model()
result2 = v1.get_horsepower()
result3 = v1.get_color()
print (v1.print_info())

v1.set_color('Pink')
v1.set_color('BLACK')
v1.owner = 'Vasyok'
result3 = v1.get_color()
print (v1.print_info())
